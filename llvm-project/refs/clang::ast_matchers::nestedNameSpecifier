<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='5894'>/// Matches nested name specifiers.
///
/// Given
/// \code
///   namespace ns {
///     struct A { static void f(); };
///     void A::f() {}
///     void g() { A::f(); }
///   }
///   ns::A a;
/// \endcode
/// nestedNameSpecifier()
///   matches &quot;ns::&quot; and both &quot;A::&quot;</doc>
