<mbr r='llvm::bfi_detail::IrreducibleGraph::Nodes' o='192' t='std::vector&lt;IrrNode&gt;'/>
<mbr r='llvm::bfi_detail::IrreducibleGraph::Lookup' o='384' t='SmallDenseMap&lt;uint32_t, llvm::bfi_detail::IrreducibleGraph::IrrNode *, 4&gt;'/>
