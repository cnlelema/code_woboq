<dec f='llvm/llvm/include/llvm/FuzzMutate/FuzzerCLI.h' l='37' type='void llvm::handleExecNameEncodedBEOpts(llvm::StringRef ExecName)'/>
<doc f='llvm/llvm/include/llvm/FuzzMutate/FuzzerCLI.h' l='28'>/// Handle backend options that are encoded in the executable name.
///
/// Parses some common backend options out of a specially crafted executable
/// name (argv[0]). For example, a name like llvm-foo-fuzzer--aarch64-gisel
/// might set up an AArch64 triple and the Global ISel selector. This should be
/// called *before* parseFuzzerCLOpts if calling both.
///
/// This is meant to be used for environments like OSS-Fuzz that aren&apos;t capable
/// of passing in command line arguments in the normal way.</doc>
<def f='llvm/llvm/lib/FuzzMutate/FuzzerCLI.cpp' l='38' ll='72' type='void llvm::handleExecNameEncodedBEOpts(llvm::StringRef ExecName)'/>
<use f='llvm/llvm/tools/llvm-isel-fuzzer/llvm-isel-fuzzer.cpp' l='123' u='c' c='LLVMFuzzerInitialize'/>
