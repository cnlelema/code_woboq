<dec f='llvm/llvm/lib/Target/Mips/MipsSEInstrInfo.h' l='113' type='void llvm::MipsSEInstrInfo::expandCvtFPInt(llvm::MachineBasicBlock &amp; MBB, MachineBasicBlock::iterator I, unsigned int CvtOpc, unsigned int MovOpc, bool IsI64) const'/>
<doc f='llvm/llvm/lib/Target/Mips/MipsSEInstrInfo.h' l='103'>/// Expand pseudo Int-to-FP conversion instructions.
  ///
  /// For example, the following pseudo instruction
  ///  PseudoCVT_D32_W D2, A5
  /// gets expanded into these two instructions:
  ///  MTC1 F4, A5
  ///  CVT_D32_W D2, F4
  ///
  /// We do this expansion post-RA to avoid inserting a floating point copy
  /// instruction between MTC1 and CVT_D32_W.</doc>
<def f='llvm/llvm/lib/Target/Mips/MipsSEInstrInfo.cpp' l='888' ll='910' type='void llvm::MipsSEInstrInfo::expandCvtFPInt(llvm::MachineBasicBlock &amp; MBB, MachineBasicBlock::iterator I, unsigned int CvtOpc, unsigned int MovOpc, bool IsI64) const'/>
