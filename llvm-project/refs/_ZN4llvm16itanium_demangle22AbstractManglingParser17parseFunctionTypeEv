<def f='llvm/llvm/include/llvm/Demangle/ItaniumDemangle.h' l='3212' ll='3275' type='llvm::itanium_demangle::Node * llvm::itanium_demangle::AbstractManglingParser::parseFunctionType()'/>
<doc f='llvm/llvm/include/llvm/Demangle/ItaniumDemangle.h' l='3204'>// &lt;function-type&gt; ::= [&lt;CV-qualifiers&gt;] [&lt;exception-spec&gt;] [Dx] F [Y] &lt;bare-function-type&gt; [&lt;ref-qualifier&gt;] E
//
// &lt;exception-spec&gt; ::= Do                # non-throwing exception-specification (e.g., noexcept, throw())
//                  ::= DO &lt;expression&gt; E # computed (instantiation-dependent) noexcept
//                  ::= Dw &lt;type&gt;+ E      # dynamic exception specification with instantiation-dependent types
//
// &lt;ref-qualifier&gt; ::= R                   # &amp; ref-qualifier
// &lt;ref-qualifier&gt; ::= O                   # &amp;&amp; ref-qualifier</doc>
