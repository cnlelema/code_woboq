<fun r='_ZN3lld3elf10StringRefZC1EN4llvm9StringRefE'/>
<mbr r='lld::elf::StringRefZ::Data' o='0' t='const char *'/>
<mbr r='lld::elf::StringRefZ::Size' o='64' t='const uint32_t'/>
