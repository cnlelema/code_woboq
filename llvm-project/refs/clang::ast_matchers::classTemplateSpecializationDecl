<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='410'>/// Matches C++ class template specializations.
///
/// Given
/// \code
///   template&lt;typename T&gt; class A {};
///   template&lt;&gt; class A&lt;double&gt; {};
