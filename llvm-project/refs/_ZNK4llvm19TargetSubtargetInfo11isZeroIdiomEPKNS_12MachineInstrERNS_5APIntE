<def f='llvm/llvm/include/llvm/CodeGen/TargetSubtargetInfo.h' l='156' ll='158' type='bool llvm::TargetSubtargetInfo::isZeroIdiom(const llvm::MachineInstr * MI, llvm::APInt &amp; Mask) const'/>
<use f='llvm/llvm/include/llvm/CodeGen/TargetSubtargetInfo.h' l='168' u='c' c='_ZNK4llvm19TargetSubtargetInfo20isDependencyBreakingEPKNS_12MachineInstrERNS_5APIntE'/>
<doc f='llvm/llvm/include/llvm/CodeGen/TargetSubtargetInfo.h' l='147'>/// Returns true if MI is a dependency breaking zero-idiom instruction for the
  /// subtarget.
  ///
  /// This function also sets bits in Mask related to input operands that
  /// are not in a data dependency relationship.  There is one bit for each
  /// machine operand; implicit operands follow explicit operands in the bit
  /// representation used for Mask.  An empty (i.e. a mask with all bits
  /// cleared) means: data dependencies are &quot;broken&quot; for all the explicit input
  /// machine operands of MI.</doc>
