<dec f='llvm/clang/include/clang/Parse/Parser.h' l='2310' type='clang::Parser::TPResult clang::Parser::isExpressionOrTypeSpecifierSimple(tok::TokenKind Kind)'/>
<doc f='llvm/clang/include/clang/Parse/Parser.h' l='2300'>/// Based only on the given token kind, determine whether we know that
  /// we&apos;re at the start of an expression or a type-specifier-seq (which may
  /// be an expression, in C++).
  ///
  /// This routine does not attempt to resolve any of the trick cases, e.g.,
  /// those involving lookup of identifiers.
  ///
  /// \returns \c TPR_true if this token starts an expression, \c TPR_false if
  /// this token starts a type-specifier-seq, or \c TPR_ambiguous if it cannot
  /// tell.</doc>
<use f='llvm/clang/lib/Parse/ParseDecl.cpp' l='4347' u='c' c='_ZN5clang6Parser18ParseEnumSpecifierENS_14SourceLocationERNS_8DeclSpecERKNS0_18ParsedTemplateInfoENS_15AccessSpecifierENS0_15DeclSpecContextE'/>
