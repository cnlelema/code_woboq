<fun r='_ZNK5clang32SubstNonTypeTemplateParmPackExpr16getParameterPackEv'/>
<fun r='_ZNK5clang32SubstNonTypeTemplateParmPackExpr24getParameterPackLocationEv'/>
<fun r='_ZNK5clang32SubstNonTypeTemplateParmPackExpr15getArgumentPackEv'/>
