<doc f='llvm/clang/include/clang/Format/Format.h' l='1402'>/// A regular expression matching macros that start a block.
  /// \code
  ///    # With:
  ///    MacroBlockBegin: &quot;^NS_MAP_BEGIN|\
  ///    NS_TABLE_HEAD$&quot;
  ///    MacroBlockEnd: &quot;^\
  ///    NS_MAP_END|\
  ///    NS_TABLE_.*_END$&quot;
  ///
  ///    NS_MAP_BEGIN
  ///      foo();
  ///    NS_MAP_END
  ///
  ///    NS_TABLE_HEAD
  ///      bar();
  ///    NS_TABLE_FOO_END
  ///
  ///    # Without:
  ///    NS_MAP_BEGIN
  ///    foo();
  ///    NS_MAP_END
  ///
  ///    NS_TABLE_HEAD
  ///    bar();
  ///    NS_TABLE_FOO_END
  /// \endcode</doc>
