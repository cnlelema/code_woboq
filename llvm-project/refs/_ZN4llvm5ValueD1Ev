<dec f='llvm/llvm/include/llvm/IR/Value.h' l='211' type='void llvm::Value::~Value()'/>
<doc f='llvm/llvm/include/llvm/IR/Value.h' l='206'>/// Value&apos;s destructor should be virtual by design, but that would require
  /// that Value and all of its subclasses have a vtable that effectively
  /// duplicates the information in the value ID. As a size optimization, the
  /// destructor has been protected, and the caller should manually call
  /// deleteValue.</doc>
<doc f='llvm/llvm/include/llvm/IR/Value.h' l='211'>// Use deleteValue() to delete a generic Value.</doc>
<def f='llvm/llvm/lib/IR/Value.cpp' l='72' ll='96' type='void llvm::Value::~Value()'/>
