<mbr r='llvm::MachineMemOperand::AtomicInfo' o='288' t='llvm::MachineMemOperand::MachineAtomicInfo'/>
<mbr r='llvm::MachineMemOperand::AAInfo' o='320' t='llvm::AAMDNodes'/>
<mbr r='llvm::MachineMemOperand::Ranges' o='512' t='const llvm::MDNode *'/>
