<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='5711'>/// Matches record types (e.g. structs, classes).
///
/// Given
/// \code
///   class C {};
///   struct S {};
///
///   C c;
///   S s;
/// \endcode
///
/// \c recordType() matches the type of the variable declarations of both \c c
/// and \c s.</doc>
