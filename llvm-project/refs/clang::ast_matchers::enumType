<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='5670'>/// Matches enum types.
///
/// Given
/// \code
///   enum C { Green };
///   enum class S { Red };
///
///   C c;
///   S s;
/// \endcode
//
/// \c enumType() matches the type of the variable declarations of both \c c and
/// \c s.</doc>
