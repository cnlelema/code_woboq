<mbr r='clang::format::FormatStyle::BraceWrappingFlags::AfterEnum' o='24' t='bool'/>
<mbr r='clang::format::FormatStyle::BraceWrappingFlags::AfterFunction' o='32' t='bool'/>
<mbr r='clang::format::FormatStyle::BraceWrappingFlags::AfterNamespace' o='40' t='bool'/>
<mbr r='clang::format::FormatStyle::BraceWrappingFlags::AfterObjCDeclaration' o='48' t='bool'/>
<mbr r='clang::format::FormatStyle::BraceWrappingFlags::AfterStruct' o='56' t='bool'/>
<mbr r='clang::format::FormatStyle::BraceWrappingFlags::AfterUnion' o='64' t='bool'/>
<mbr r='clang::format::FormatStyle::BraceWrappingFlags::AfterExternBlock' o='72' t='bool'/>
<mbr r='clang::format::FormatStyle::BraceWrappingFlags::BeforeCatch' o='80' t='bool'/>
<mbr r='clang::format::FormatStyle::BraceWrappingFlags::BeforeElse' o='88' t='bool'/>
