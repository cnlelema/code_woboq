<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='1234'>/// Matches unresolved member expressions.
///
/// Given
/// \code
///   struct X {
///     template &lt;class T&gt; void f();
///     void g();
///   };
///   template &lt;class T&gt; void h() { X x; x.f&lt;T&gt;(); x.g(); }
/// \endcode
/// unresolvedMemberExpr()
///   matches x.f&lt;T&gt;</doc>
