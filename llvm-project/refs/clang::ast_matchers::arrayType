<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='5269'>/// Matches all kinds of arrays.
///
/// Given
/// \code
///   int a[] = { 2, 3 };
///   int b[4];
///   void f() { int c[a[0]]; }
/// \endcode
/// arrayType()
///   matches &quot;int a[]&quot;, &quot;int b[4]&quot; and &quot;int c[a[0]]&quot;;</doc>
