<fun r='_ZN5clang20TemplateParmPositionC1Ev'/>
<fun r='_ZNK5clang20TemplateParmPosition8getDepthEv'/>
<fun r='_ZN5clang20TemplateParmPosition8setDepthEj'/>
<fun r='_ZNK5clang20TemplateParmPosition11getPositionEv'/>
<fun r='_ZN5clang20TemplateParmPosition11setPositionEj'/>
<fun r='_ZNK5clang20TemplateParmPosition8getIndexEv'/>
