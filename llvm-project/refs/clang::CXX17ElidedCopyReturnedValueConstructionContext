<fun r='_ZNK5clang47CXX17ElidedCopyReturnedValueConstructionContext23getCXXBindTemporaryExprEv'/>
<fun r='_ZN5clang47CXX17ElidedCopyReturnedValueConstructionContext7classofEPKNS_19ConstructionContextE'/>
