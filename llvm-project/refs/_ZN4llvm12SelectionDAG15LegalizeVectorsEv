<dec f='llvm/llvm/include/llvm/CodeGen/SelectionDAG.h' l='539' type='bool llvm::SelectionDAG::LegalizeVectors()'/>
<doc f='llvm/llvm/include/llvm/CodeGen/SelectionDAG.h' l='528'>/// This transforms the SelectionDAG into a SelectionDAG
  /// that only uses vector math operations supported by the target.  This is
  /// necessary as a separate step from Legalize because unrolling a vector
  /// operation can introduce illegal types, which requires running
  /// LegalizeTypes again.
  ///
  /// This returns true if it made any changes; in that case, LegalizeTypes
  /// is called again before Legalize.
  ///
  /// Note that this is an involved process that may invalidate pointers into
  /// the graph.</doc>
<def f='llvm/llvm/lib/CodeGen/SelectionDAG/LegalizeVectorOps.cpp' l='1392' ll='1394' type='bool llvm::SelectionDAG::LegalizeVectors()'/>
<use f='llvm/llvm/lib/CodeGen/SelectionDAG/SelectionDAGISel.cpp' l='851' u='c' c='_ZN4llvm16SelectionDAGISel17CodeGenAndEmitDAGEv'/>
