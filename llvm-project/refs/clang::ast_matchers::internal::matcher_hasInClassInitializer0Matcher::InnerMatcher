<doc f='llvm/clang/include/clang/ASTMatchers/ASTMatchers.h' l='618'>/// Matches non-static data members that have an in-class initializer.
///
/// Given
/// \code
///   class C {
///     int a = 2;
///     int b = 3;
///     int c;
///   };
/// \endcode
/// fieldDecl(hasInClassInitializer(integerLiteral(equals(2))))
///   matches &apos;int a;&apos; but not &apos;int b;&apos;.
/// fieldDecl(hasInClassInitializer(anything()))
///   matches &apos;int a;&apos; and &apos;int b;&apos; but not &apos;int c;&apos;.</doc>
